package com.utils;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;

public class LoginPageElmt extends BaseClass{
	static WebElement username = driver.findElement(By.name("username"));
	static WebElement password = driver.findElement(By.name("username"));
	static WebElement submitbtn = driver.findElement(By.xpath("//*[@type='submit']"));
	static WebElement logoutbtn = driver.findElement(By.xpath("//*[contains(@href,'logout')]"));
}
